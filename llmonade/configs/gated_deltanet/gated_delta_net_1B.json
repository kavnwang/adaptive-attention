{
    "attn_mode": "chunk",
    "bos_token_id": 1,
    "eos_token_id": 2,
    "pad_token_id": 2,
    "expand_v": 2,
    "fuse_cross_entropy": true,
    "hidden_act": "swish",
    "hidden_ratio": 4,
    "hidden_size": 2048,
    "initializer_range": 0.006,
    "intermediate_size": null,
