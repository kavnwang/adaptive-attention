    "num_heads": 12,
    "head_dim": 128,
    "num_hidden_layers": 24,
    "norm_eps": 1e-06,
    "norm_first": false,
    "qk_activation": "silu",
    "qk_norm": "l2",
    "tie_word_embeddings": false,
    "use_beta": true,
    "use_cache": true,
    "use_gate": true,
    "use_output_norm": true,
    "use_short_conv": true,
    "conv_size": 4,
    "vocab_size": 32000,
    "torch_dtype": "float32"
